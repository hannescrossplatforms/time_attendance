.yui-h-slider,.yui-v-slider
{
    position:relative;
}

.yui-h-slider .yui-slider-thumb,.yui-v-slider .yui-slider-thumb
{
    position:absolute;
    cursor:default;
}

.yui-skin-sam .yui-h-slider
{
    background:url(/images/frontend/bg-h.gif) no-repeat 5px 0;height:28px;width:228px;
}

.yui-skin-sam .yui-h-slider .yui-slider-thumb
{
    top:4px;
}

.yui-skin-sam .yui-v-slider
{
    background:url(/images/frontend/bg-v.gif) no-repeat 12px 0;height:228px;width:48px;
}

