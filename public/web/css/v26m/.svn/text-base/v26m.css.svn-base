.submit2{
	-moz-border-radius:5px;
	-webkit-border-radius:5px;
	border-radius:5px;
}

.submit2{
	width:45%;
	height:40px;
	background-color:#9796fe;
	border:solid 1px #5b59dd;
	font-size:11px;
	font-weight:bold;
	color:#17167e;
/*	text-shadow:rgba(177, 176, 254, 1) -1px 1px 0px;*/
	float:right;
	margin-bottom:10px;
	cursor:pointer;
	-webkit-appearance: none;
        white-space: normal;
}
.submit2:hover, .submit2:active{
	background:#8281f7;
}
.submit2.l-spacing{
	letter-spacing:-1px;
}