<script type="text/javascript">
YAHOO.util.Event.onDOMReady( function () {
  var Dom = YAHOO.util.Dom,
    Event = YAHOO.util.Event;


  var pb = new YAHOO.widget.ProgressBar({value:0,minValue:0,maxValue:100}).render('basic');



  var widthSlider = YAHOO.widget.Slider.getHorizSlider('sliderWidth',
        'sliderWidthThumb', 0, 200, 0
  );
  widthSlider.animate = true;
  widthSlider.setValue(99);
  widthSlider.subscribe("change", function(offsetFromStart) {
    var value = Math.round(offsetFromStart);
    var valuearea="width";
    var widthfield="width-converted-value";

     var valnode = Dom.get(valuearea);
     var fld = Dom.get(widthfield);

     valnode.innerHTML = value;

     fld.value = Math.round(value / 2);

    //Dom.get('width').innerHTML = value;
    //Dom.get('width-converted-value').input = fld.value;
  });



  widthSlider.subscribe("slideEnd", function() {
    var value = widthSlider.getValue();
    pb.set('width',value);
  });


  var heightSlider = YAHOO.widget.Slider.getHorizSlider('sliderHeight',
        'sliderHeightThumb', 0, 200, 0
  );
  heightSlider.animate = true;
  heightSlider.setValue(99);
  heightSlider.subscribe("change", function(offsetFromStart) {
    var value = Math.round(offsetFromStart);
    var valuearea="height";
    var heightfield="height-converted-value";

     var valnode = Dom.get(valuearea);
     var fld = Dom.get(heightfield);

     valnode.innerHTML = value;

     fld.value = Math.round(value / 2);
     Dom.get('height').innerHTML = value;

  });

  heightSlider.subscribe("slideEnd", function() {
    var value = Math.round(heightSlider.getValue() / 0.5);
    pb.set('height',value);
  });

});
</script>

